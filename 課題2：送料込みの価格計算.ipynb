   "source": [
    "price_ex_tax = int(input(\"税抜価格を入力してください：\"))\n",
    "\n",
    "price_in_tax = round(price_ex_tax * 1.1,0)\n",
    "\n",
    "if price_in_tax >= 2000:\n",
    "    print(\"送料は無料です\")\n",
    "else:\n",
    "    print(f\"送料として350円かかります\")\n",
    "    price_in_tax += 350\n",
    "\n",
    "print(f\"送料込みの価格は{int(price_in_tax)}円です。\")"
   ]
  }
